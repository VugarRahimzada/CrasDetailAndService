@model CoreLayer.Results.Concrete.DataResult<List<DTOLayer.ContactUsDTOs>>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}


<h1>Contact Us</h1>


    <table class="table table-bordered">
        <tr>
            <th>Full Name</th>
            <th>Email</th>
            <th>Subject</th>
            <th>Message</th>
        </tr>

        @foreach (var item in Model.Data)
        {
            <tr>
                <td>@item.Name @item.Surname</td>
                <td>@item.Email</td>
                <td>@item.Subject</td>
                <td>@item.Message</td>
            </tr>
        }
    </table>



