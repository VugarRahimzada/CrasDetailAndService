@model CoreLayer.Results.Concrete.SuccessDataResult<DTOLayer.BlogDTOs>




<div class="ak-height-75 ak-height-lg-80"></div>
<section class="container">
    <div class="blog-feature">
        <div class="feature-content" data-aos="fade-right" data-aos-delay="600">
            <p class="data">@Model.Data.CreateTime.ToString("dd.MM.yyyy")</p>
            <h4 class="title">@Model.Data.Title</h4>
            <p class="desp">
                     @Model.Data.Text
            </p>
            <a href="~/Blog/BlogDetail/@Model.Data.Id" class="more-btn">Davamını Oxu</a>
        </div>
        <div class="feature-img" data-aos="fade-left" data-aos-delay="600">
            <img src="~/web-cras-three.vercel.app/assets/img/blog-show.jpg" class="ak-bg" alt="...">
        </div>
    </div>
</section>